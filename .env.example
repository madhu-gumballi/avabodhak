# Copy this file to `.env` in the project root and fill the values

# Google Custom Search JSON API key (used by scripts/fetch-images.mjs)
GOOGLE_CSE_API_KEY=YOUR_GOOGLE_CSE_API_KEY

# Google Custom Search Engine ID (cx)
GOOGLE_CSE_CX=YOUR_CSE_CX_ID

# Google Analytics 4 Measurement ID (format: G-XXXXXXXXXX)
# Get this from: https://analytics.google.com/ → Admin → Data Streams → Web Stream Details
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Feature flags
VITE_FEATURE_TTS=true
